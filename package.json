  },
  "keywords": [
    "markdown-it",
    "markdown",
    "parser",
    "generator",
    "renderer"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib/"
  ],
  "scripts": {
    "test": "jest --coverage --verbose --silent=false",
    "test:watch": "jest --watch --verbose --silent=false",
