    "build:clean": "rm -rf lib",
    "build:dev": "npm run build:dev:clean && node scripts/development.js",
    "build:dev:clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "author": {
    "name": "moki",
