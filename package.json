    "@types/jest": "^27.5.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.11.19",
    "@yandex-cloud/eslint-config": "^1.0.1",
    "@yandex-cloud/prettier-config": "^1.0.0",
    "esbuild": "^0.17.16",
    "esbuild-jest": "^0.5.0",
