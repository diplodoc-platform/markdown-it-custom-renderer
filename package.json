  "description": "extendable custom renderer for markdown-it",
  "homepage": "https://github.com/diplodoc-platform/markdown-it-custom-renderer",
  "bugs": {
    "url": "https://github.com/diplodoc-platform/markdown-it-custom-renderer/issues",
